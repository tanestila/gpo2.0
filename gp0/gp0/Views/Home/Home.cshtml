@model IEnumerable<Document>
@{
    ViewData["Title"] = "Home Page";
    Layout = "HomeLayout";
}
<table class="table">
    <thead class="thead-light">
    <tr>
        <th scope="col">#</th>
        <th scope="col">Отправитель</th>
        <th scope="col">Дата</th>
        <th scope="col">Сообщение</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var doc in Model)
    {
        <tr>
            <th scope="row"></th>
            <td>@doc.sender</td>
            <td>@doc.date</td>
            <td>@doc.text</td>
        </tr>
    }
    </tbody>
</table>



